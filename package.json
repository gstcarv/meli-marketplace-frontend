{
    "name": "@meli/marketplace",
    "license": "MIT",
    "private": true,
    "scripts": {
        "build": "turbo build",
        "start": "turbo run dev --parallel",
        "test": "turbo run test",
        "test:coverage": "turbo run test:coverage",
        "lint": "turbo lint",
        "ts-check": "turbo ts-check",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "devDependencies": {
        "prettier": "^3.2.5",
        "turbo": "^2.1.3",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=18"
    },
    "packageManager": "yarn@1.22.22",
    "workspaces": [
        "apps/*",
        "packages/*"
    ]
}
